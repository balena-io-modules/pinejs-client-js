{
  "name": "pinejs-client-core",
  "version": "6.14.0",
  "description": "This module provides the core tools to enable easier interaction with the pinejs API.",
  "browser": "es2015",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:balena-io-modules/pinejs-client-js.git"
  },
  "author": "Pagan Gazzard <page@balena.io>",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "lint": "balena-lint --typescript src test",
    "pretest": "npm run build",
    "test": "mocha",
    "posttest": "npm run lint",
    "test6": "npm run build && mocha",
    "build-es2015": "tsc --target es2015 --outDir es2015",
    "build-es2018": "tsc --target es2018 --outDir es2018",
    "build-types": "tsc --emitDeclarationOnly --outDir .",
    "build": "npm run build-es2015 && npm run build-es2018 && npm run build-types",
    "prepublish": "require-npm4-to-publish",
    "prepack": "npm run build",
    "prettify": "balena-lint --typescript --fix src test"
  },
  "license": "MIT",
  "dependencies": {
    "@balena/es-version": "^1.0.1"
  },
  "devDependencies": {
    "@balena/lint": "^5.4.2",
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.6",
    "husky": "^4.3.8",
    "lint-staged": "^12.3.7",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "require-npm4-to-publish": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "mocha": {
    "reporter": "spec",
    "recursive": true,
    "require": "ts-node/register/transpile-only",
    "_": "test/*.ts"
  },
  "versionist": {
    "publishedAt": "2023-12-05T13:05:12.245Z"
  }
}
