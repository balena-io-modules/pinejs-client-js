    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "coffee-script": "~1.12.7",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "lodash": "^4.17.15",
    "mocha": "^6.2.3",
    "require-npm4-to-publish": "^1.0.0",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
